  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#d467b9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#932d89",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords",
      "scope": ["keyword.control", "keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "#932d89"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "punctuation.definition.string"],
      "settings": {
        "foreground": "#541f54"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#ba39ad"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#541f54"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#742774"
      }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#371236",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#371236"
      }
    },
    {
      "name": "Variable Properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#742774"
      }
    },
    {
      "name": "Classes and Types",
      "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
      "settings": {
        "foreground": "#932d89"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#bb35a4"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#bb35a4"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace", "punctuation.definition.block"],
      "settings": {
        "foreground": "#742774"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#932d89"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ba39ad",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "support.type.vendored.property-name"],
      "settings": {
        "foreground": "#742774"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value", "support.constant.color"],
      "settings": {
        "foreground": "#541f54"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#ba39ad"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json", "string.json support.type.property-name"],
      "settings": {
        "foreground": "#932d89"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#932d89",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#541f54",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ba39ad",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#bb35a4"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#742774"
      }
    },
    {
      "name": "YAML Keys",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#932d89"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#bb35a4"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ba39ad"
      }
    },
    {
      "name": "Invalid/Deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ba39ad"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#932d89"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#d467b9"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ba39ad"
      }
    }
  ]
}
